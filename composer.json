{
    "name": "frankyframework/franky-skeleton-application",
    "description": "Skeleton Application for Franky Framework applications",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.0",
        "phpmailer/phpmailer": "~6.0",
        "mobiledetect/mobiledetectlib": "^2.8",
        "frankyframework/franky-haxor": "dev-master",
        "frankyframework/franky-core": "dev-master",
        "frankyframework/franky-database": "dev-master",
        "frankyframework/franky-form": "dev-master",
        "frankyframework/franky-filesystem": "dev-master",
        "frankyframework/franky-schema": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
            "psr-4": {
                "modulos\\base\\": "modulos/base/",
                "modulos\\blog\\": "modulos/blog/",
                "modulos\\galeria\\": "modulos/galeria/",
                "modulos\\seo\\": "modulos/seo/",
                "modulos\\developer\\": "modulos/developer/",
                "modulos\\sociallogin\\": "modulos/sociallogin/",
                "modulos\\ecommerce\\": "modulos/ecommerce/",
                "modulos\\franky\\": "modulos/franky/"
            }
    }
}
