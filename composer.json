{
    "name": "sombrerodepaja/franky-skeleton-application",
    "description": "Skeleton Application for Franky Framework applications",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.0",
        "phpmailer/phpmailer": "~6.0",
        "mobiledetect/mobiledetectlib": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
            "psr-4": {
                "base\\": "modulos/base/",
                "blog\\": "modulos/blog/",
                "galeria\\": "modulos/galeria/",
                "seo\\": "modulos/seo/",
                "developer\\": "modulos/developer/",
                "sociallogin\\": "modulos/sociallogin/",
                "ecommerce\\": "modulos/ecommerce/",
                "franky\\": "modulos/franky/"
            }
    }
}
